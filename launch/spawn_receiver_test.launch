<?xml version="1.0" ?>
<launch>
    <arg name="x"               default="0.0"/>
    <arg name="y"               default="0.0"/>
    <arg name="z"               default="0.0"/>\

	<param name="robot_description" command="$(find xacro)/xacro --inorder $(find arva_sim)/urdf/receiver.urdf.xacro channels:=4 frame_id:=arva_receiver sensor:=arva_receiver sensor_name:=receiver1"/>
	<node name="spawn_artva_receiver" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -x $(arg x) -y $(arg y) -z $(arg z) -model artva_receiver" respawn="false" output="screen">
	</node>

	<include file="$(find arva_sim)/launch/rviz.launch"/>


</launch>

